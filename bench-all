#!/bin/bash

FORKS=1
MEASUREMENT_TIME=2

for n in 1; do
  for s in 1 10 100 1000; do
    echo ./bench --forks ${FORKS} --measurement-time ${MEASUREMENT_TIME} deser --number $n --size $s
    ./bench --forks ${FORKS} --measurement-time ${MEASUREMENT_TIME} deser --number $n --size $s > output/deser-$n-$s.txt
    echo ./bench --forks ${FORKS} --measurement-time ${MEASUREMENT_TIME} ser --number $n --size $s
    ./bench --forks ${FORKS} --measurement-time ${MEASUREMENT_TIME}   ser --number $n --size $s > output/ser-$n-$s.txt
  done
done

